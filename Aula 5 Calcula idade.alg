algoritmo "Creuza01"
// Função :
// Autor :
// Data : 30/08/2022
// Seção de Declarações 
var
   ano_atual, ano_nasc, idade: inteiro
inicio
      Escreval("Em que ano nós estamos? ")
      Leia(ano_atual)
      Escreval("Em que ano eu nasci? ")
      Leia(ano_nasc)
      idade <- ano_atual - ano_nasc
      Escreval("Minha idade é:", idade, "anos")
fimalgoritmo

