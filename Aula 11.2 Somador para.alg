algoritmo "Somador"
// Função :
// Autor :
// Data : 01/09/2022
// Seção de Declarações 
var
   n: inteiro
   c: inteiro
   s: inteiro
inicio
      S <- 0
      Para c <- 1 ate 5 faca
      Escreval("Digite umnúmero: ")
      leia(n)
      s <- s + n

      fimpara
      escreval("A soma dos valore é: ", s)
fimalgoritmo
