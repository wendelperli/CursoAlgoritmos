algoritmo "Conversao temperatura"
// Função :
// Autor :
// Data : 30/08/2022
// Seção de Declarações 
var
   F, C: real
inicio
      Escreva("Digite a temperatura em F: ")
      leia(F)
      C <- (F -32) / 1.8
      Escreva("A temperatura em Celcius é: ", C:4:1, " graus")
fimalgoritmo
